{
  "name": "miwo-frontend",
  "scripts": {
    "start": "vite",
    "serve": "vite dev",
    "build": "vite build",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .ts,vue --ignore-path .gitignore .",
    "prettier": "prettier -w -u .",
    "test": "npm run test:unit:typescript && npm run test:e2e:typescript",
    "test:unit:typescript": "jest --config=jest.unit.config.js",
    "test:unit:typescript:coverage": "jest --config=jest.unit.config.js --coverage",
    "test:e2e:typescript": "start-server-and-test start http-get://localhost:3000 e2e:typescript",
    "e2e:typescript": "cypress run --headed --browser chrome && cypress run --headed --config viewportWidth=320,viewportHeight=480 --browser chrome"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@headlessui/vue": "^1.3.0",
    "@heroicons/vue": "^1.0.1",
    "@tailwindcss/forms": "^0.3.3",
    "axios": "^0.21.1",
    "vue": "3.1.3",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/compiler-sfc": "3.1.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "^10.2.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^26.6.3",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.12.5",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5",
    "vite": "^2.3.8",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "14"
  }
}
